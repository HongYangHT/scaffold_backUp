				<div v-for="item in editLayout">
					<div class="u-group" v-if="item.type == 'input'">
						<label>{{item.name}}：</label>
						<div class="u-psc-input-group">
							<input type="text" class="u-psc-input" v-model="item.value" />
						</div>
					</div>
					<div class="u-group" v-if="item.type == 'textarea'">
						<label>{{item.name}}：</label>
						<div class="u-psc-input-group">
							<textarea v-model="item.value" class="u-psc-textarea">{{item.value}}</textarea>
						</div>
					</div>
					<div class="u-group" v-if="item.type == 'select'">
						<label>{{item.name}}：</label>
						<div class="u-psc-input-group">
							<input type="text" class="u-psc-input" v-model="item.value" />
							<select class="u-psc-input" v-model="item.value" >
								<option v-for="option in item.options" value="{{option.value}}">
									<span>{{option.name}}</span>
								</option>
							</select>
						</div>
					</div>
				</div>
				<div v-for="props in editProperties">
					<div class="m-group" v-for="item in props">
						<div v-if="item | checkType item 'object'">
							<div v-for="prop in item">
								<div class="u-group" v-if="prop.input">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<input type="text" class="u-psc-input J_colorpicker" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)" v-if="prop.changeColor" v-on:focus="bindColorPicker($event)" v-on:click="bindColorPicker($event)" />
										<input type="text" class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)" v-else/>
										<label class="u-upload J_upload" v-if="prop.needUpload" v-on:click="uploadImg">
											<input type="file" name="{{prop.key}}" style="display:none;">
										</label>
										<form action="http://pub.mail.163.com/uploader/ajax/uploadBinary.do" method="post" enctype="multipart/form-data" target="fileUpload" class="compress J_uploadForm" v-if="prop.needUpload">
											<div class="u-upload J_upload">
												<input type="file" name="files" class="u_uploadOneImg J_uploadOneImg" data-type="compress" data-imagePath=""  data-localPath="" v-on:change="uploadImg($event)"/>
												<i class="fa fa-upload" aria-hidden="true"></i>
											</div>
										</form>
									</div>
								</div>
								<div class="u-group" v-if="prop.textArea">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<textarea v-model="prop.value" class="u-psc-textarea" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)">{{prop.value}}</textarea>
									</div>
								</div>
								<div class="u-group" v-if="prop.select">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<input type="text" class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)"/>
										<select class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)">
											<option v-for="option in prop.options" value="{{option.value}}">
												<span>{{option.name}}</span>
											</option>
										</select>
									</div>
								</div>
							</div>
						</div>
						<div v-if="item | checkType item 'array'">
							<div v-for="prop in item">
								<div class="u-group" v-if="prop.input">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<input type="text" class="u-psc-input J_colorpicker" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)" v-if="prop.changeColor" v-on:focus="bindColorPicker($event)" v-on:click="bindColorPicker($event)"/>
										<input type="text" class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)" v-else/>
										<label class="u-upload J_upload" v-if="prop.needUpload" v-on:click="uploadImg">
											<input type="file" name="{{prop.key}}" style="display:none;">
										</label>
										<form action="http://pub.mail.163.com/uploader/ajax/uploadBinary.do" method="post" enctype="multipart/form-data" target="fileUpload" class="compress J_uploadForm" v-if="prop.needUpload">
											<div class="u-upload J_upload">
												<input type="file" name="files" class="u_uploadOneImg J_uploadOneImg" data-type="compress" data-imagePath=""  data-localPath="" v-on:change="uploadImg($event)"/>
												<i class="fa fa-upload" aria-hidden="true"></i>
											</div>
										</form>
									</div>
								</div>
								<div class="u-group" v-if="prop.textArea">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<textarea v-model="prop.value" class="u-psc-textarea" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)">{{prop.value}}</textarea>
									</div>
								</div>
								<div class="u-group" v-if="prop.select">
									<label>{{prop.name}}：</label>
									<div class="u-psc-input-group">
										<input type="text" class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)"/>
										<select class="u-psc-input" v-model="prop.value" v-on:focus="focusInput($event,prop.key,id)" v-on:blur="blurInput($event,prop.key,id)">
											<option v-for="option in prop.options" value="{{option.value}}">
												<span>{{option.name}}</span>
											</option>
										</select>
									</div>
								</div>
							</div>
						</div>
						<div v-else>
							<div class="u-group" v-if="item.input">
								<label>{{item.name}}：</label>
								<div class="u-psc-input-group">
									<input type="text" class="u-psc-input J_colorpicker" v-model="item.value" v-on:focus="focusInput($event,item.key,id)" v-on:blur="blurInput($event,item.key,id)" v-if="item.changeColor" v-on:focus="bindColorPicker($event)" v-on:click="bindColorPicker($event)"/>
									<input type="text" class="u-psc-input" v-model="item.value" v-on:focus="focusInput($event,item.key,id)" v-on:blur="blurInput($event,item.key,id)" v-else/>
									<!-- <label class="u-upload J_upload" v-if="item.needUpload" v-on:click="uploadImg">
										<input type="file" name="{{item.key}}" style="display:none;">
									</label> -->
									<form action="http://pub.mail.163.com/uploader/ajax/uploadBinary.do" method="post" enctype="multipart/form-data" target="fileUpload" class="compress J_uploadForm" v-if="item.needUpload">
										<div class="u-upload J_upload">
											<input type="file" name="files" class="u_uploadOneImg J_uploadOneImg" data-type="compress" data-imagePath=""  data-localPath="" v-on:change="uploadImg($event)"/>
											<i class="fa fa-upload" aria-hidden="true"></i>
										</div>
									</form>
								</div>
							</div>
							<div class="u-group" v-if="item.textArea">
								<label>{{item.name}}：</label>
								<div class="u-psc-input-group">
									<textarea  class="u-psc-textarea" v-model="item.value" v-on:focus="focusInput($event,item.key,id)" v-on:blur="blurInput($event,item.key,id)">{{item.value}}</textarea>
								</div>
							</div>
							<div class="u-group" v-if="item.select">
								<label>{{item.name}}：</label>
								<div class="u-psc-input-group">
									<input type="text" class="u-psc-input" v-model="item.value" v-on:focus="focusInput($event,item.key,id)" v-on:blur="blurInput($event,item.key,id)"/>
									<select class="u-psc-input" v-model="item.value" v-on:focus="focusInput($event,item.key,id)" v-on:blur="blurInput($event,item.key,id)">
										<option v-for="option in item.options" value="{{option.value}}">
											<span>{{option.name}}</span>
										</option>
									</select>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>